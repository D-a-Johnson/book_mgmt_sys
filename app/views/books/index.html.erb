 <table style="width:50%">
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Author</th>
    <th>Books Remaining</th>
    <th>Cost per Day</th>
  </tr>
  <% @books.each do |book| %>
  <tr>
    <td><%= book.id %></td>
    <td><%= book.title %></td>
    <td><%= book.author %></td>
    <td><%= book.total_books %></td>
    <td><%= book.cost_day %></td>
  </tr>
  <% end %>
</table>
